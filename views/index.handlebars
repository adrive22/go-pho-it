<h1>GET-SOME-PHO!</h1>

<main>
  <div class="container">
    <div class="row">
      <div class="col s6">
        {{#each pho}}
          {{#unless this.devoured}}
            <div>
              <form action="/pho/{{this.id}}?_method=PUT" method="POST">
                <button type="submit" class='phoAvailable'> {{this.pho_name}}</button>
              </form>
            </div>
          {{/unless}}
        {{/each}}
      </div>
